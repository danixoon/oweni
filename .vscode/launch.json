  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Test",
      "program": "${workspaceFolder}/app/test.js",
      "request": "launch",
      "args": ["testForm"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node"
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "pwa-node",
      "restart": true
    },

    {
      "name": "Listen for XDebug",
      "type": "php",
      "request": "launch",
      "port": 9000
    }
  ]
}
